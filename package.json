  "engines": {
    "node": ">=10.4.0"
  },
  "keywords": [],
  "author": "nerix",
  "license": "MIT",
  "dependencies": {
    "chance": "^1.1.6",
    "debug": "^4.1.1",
    "eventemitter3": "^4.0.4",
    "instagram-private-api": "^1.41.0",
    "mqtts": "^1.0.0",
    "socks": "^2.5.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.14",
    "@types/luxon": "^1.25.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-xor": "^1.0.8",
    "typescript": "^4.0.3"
  },
  "types": "dist/index.d.ts"
